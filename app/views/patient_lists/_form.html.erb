
<%= form_for(own_patient_list,
             :as => :own_patient_list,
             :url => user_patient_lists_path(current_user.id),
             :remote => true,
             :html => {:"data-type" => 'json'}) do |f| %>
 <% if own_patient_list.errors.any? %>
   Could not create new list
 <% end %>
 <%= f.text_field :name, placeholder: "List name", id: "new-list-name"%>
 <%= f.submit "Create List", id: "create-list" %>
<% end %>
