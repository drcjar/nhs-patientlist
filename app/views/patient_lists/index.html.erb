<h2>Patient Lists for <%= user.email %></h2>
<ul>
<% user_patient_lists.each do |list| %>
<li><%= link_to list.name, user_patient_list_path(list.user_id, list.id) %></li>
<% end %>

</ul>
<% if user == current_user %>
<%= link_to "New List",
    new_user_patient_list_path(current_user), :id => 'new-list' %>
<% end %>

